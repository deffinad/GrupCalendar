{
  "expo": {
    "name": "GrupCalendar",
    "slug": "GrupCalendar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "update": {
      "fallbackToChacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#575996"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
